{
  "name": "project-registry",
  "description": "A developer CLI that starts projects from named templates backed by shell commands.",
  "version": "0.4.1",
  "author": "Hichem Taboukouyout",
  "bin": {
    "projx": "./bin/run.js"
  },
  "bugs": "https://github.com/HichemTab-tech/project-registry/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "execa": "^9.6.1",
    "strip-ansi": "^7.1.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^5",
    "@types/mocha": "^10",
    "@types/node": "^25",
    "chai": "^6",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^11",
    "oclif": "^4",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/HichemTab-tech/project-registry",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "projx",
    "dirname": "projx",
    "commands": "./dist/commands",
    "hooks": {
      "command_not_found": "./dist/hooks/command_not_found"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "HichemTab-tech/project-registry",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
